{
  "functions": {
    "LocationReadlocations": {
      "custom": {
        "excludePatterns": [],
        "envVars": []
      },
      "handler": "modules/location/readlocations/handler.handler",
      "timeout": 6,
      "memorySize": 1024,
      "endpoints": {
        "location": {
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {},
          "requestTemplates": {
            "application/json": "{\"type\":\"$input.params('type')\"}"
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {},
              "responseModels": {},
              "responseTemplates": {
                "application/json": "#set($inputRoot = $input.path('$'))\n{\n  \"results\":[\n#foreach($hit in $inputRoot.hits.hits)\n    {\n      \"_id\" : \"$hit['_id']\",\n      \"title\" : \"$hit['_source']['title']\",\n      \"address\" : \"$hit['_source']['address']\",\n      \"description\" : \"$hit['_source']['description']\",\n      \"location\" : {\n        \"lat\" : $hit['_source']['location']['lat'],\n        \"lon\" : $hit['_source']['location']['lon']\n        },\n      \"type\" : \"$hit['_source']['type']\",\n      \"preset\" : $hit['_source']['preset']\n    }#if($foreach.hasNext),#end\n#end\n  ]\n}"
              }
            }
          }
        }
      },
      "pathFunction": "modules/location/readlocations/s-function.json",
      "name": "readlocations"
    }
  }
}